                            <a class="nav-link text-dark text-center" href="#CommunicationTalents">@Localizer["CommunicationSkillsSeperator"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark text-center" href="#WorkTalents">@Localizer["WorkSkillsSeperator"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark text-center" href="#Hobbies">@Localizer["HobbiesSeperator"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark text-center" href="#ContactMe">@Localizer["ContactMeSeperator"]</a>
