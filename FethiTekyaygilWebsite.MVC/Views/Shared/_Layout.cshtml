    <!--#Diğer CSSler-->
    <noscript id="deferred-styles">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link href="~/lib/mdbootstrap/css/mdb.min.css" rel="stylesheet" />
