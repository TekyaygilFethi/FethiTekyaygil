                        <li class="nav-item">
                            <a class="nav-link text-dark text-center" href="#FollowMe">@Localizer["FollowMeSeperator"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark text-center" href="#Support">@Localizer["SupportSeperator"]</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark text-center" href="#CV">CV</a>
                        </li>
                        <li>
                            <ul class="navbar-nav ml-auto">
                                <li class="nav-item">
                                    <div class="dropdown" align="right">
                                        <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="display:contents">
                                            <img data-src=@Url.Content("/Content/Media/Language/"+(Context.Request.Path.Value.Contains("en-US")?"EN.png":"TR.png")) alt="lang" />
                                        </button>
                                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                            <a class="dropdown-item text-center" id="TRLang"><img class="lng" src=@Url.Content("/Content/Media/Language/TR.png") alt="tr" /></a>
                                            <a class="dropdown-item text-center" id="ENLang"><img class="lng" src=@Url.Content("/Content/Media/Language/EN.png") alt="en" /></a>
                                        </div>
                                    </div>
